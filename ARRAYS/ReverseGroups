#include <iostream>
using namespace std;
int revsub(int arr[],int n,int k){
    for(int i=0;i<n;i+=k){
        int t=k;
        while(t>=0)
        {
            if((i+t-1)<n && (i+t-1)>=i)
                cout<<arr[i+t-1]<<" ";
            t--;
        }
    }
}

-------------------------------------------------------------------------------------------------------------------------------------------
int main() {
	//code
	int t;
	cin>>t;
	while(t--)
	{
	    int n,k;
	    cin>>n>>k;
	    int a[n];
	    for(int i=0;i<n;i++)
	       cin>>a[i];
	    revsub(a,n,k);
	    cout<<endl;
	}
	return 0;
}

-------------------------------------------------------------------------------------------------------------------------------------------
INPUT
5
3
1 2 3 4 5
6
2
10 20 30 40 50 60

OUTPUT
3 2 1 5 4
20 10 40 30 60 50
